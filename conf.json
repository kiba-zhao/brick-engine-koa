{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "sourceType": "module",
  "source": {
    "include": ["*.js","lib","koa"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "templates/default", 
    "encoding": "utf8",              
    "destination": "./docs/",         
    "recurse": true,
    "readme":"README.md"
  }
}
